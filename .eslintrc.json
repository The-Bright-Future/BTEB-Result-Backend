{
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  // HERE
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-console": "error",
    "no-unused-vars": "error",
    "explicit-module-boundary-types": "off",
    "ban-types": "off"
  },

  "env": {
    "node": true,
    "commonjs": true,
    "browser": true,
    "es6": true
  }
}
